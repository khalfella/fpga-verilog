# Oscillator
set_io			clk		21

# ------------ User Leds ------------------------------------------------------
set_io			leds[0]  	99
set_io			leds[1]  	98


# ------------ PMOD I/O ---------------------
set_io -pullup yes	rst_nbtn	78
set_io -pullup yes	set_nbtn	79
set_io -pullup yes	p_nbtn[0]	80
set_io -pullup yes	p_nbtn[1]	81
